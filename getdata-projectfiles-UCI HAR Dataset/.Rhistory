# Across the United States, how have emissions from coal combustion-related sources changed from 1999–2008?
# Clean workspace
rm(list=ls())
# load data
NEI <- readRDS("summarySCC_PM25.rds")
SCC <- readRDS("Source_Classification_Code.rds")
# Search for the word coal in the short.name column of SCC
source.coal <- grepl("Coal", SCC$Short.Name, fixed=TRUE)
source.coal <- SCC[source.coal,]$SCC
source.coal.NEI <- NEI[NEI$SCC %in% source.coal,]
source.coal.peryear <- tapply(source.coal.NEI$Emissions, source.coal.NEI$year,sum)
# open device
