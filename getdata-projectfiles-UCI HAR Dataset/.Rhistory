# How have emissions from motor vehicle sources changed from 1999-2008 in Baltimore City?
# Clean workspace
rm(list=ls())
# load data
NEI <- readRDS("summarySCC_PM25.rds")
SCC <- readRDS("Source_Classification_Code.rds")
# Search for the word coal in the short.name column of SCC
source.MotorVehicle <- grepl("Vehicles", SCC$Short.Name, fixed=TRUE)
source.MotorVehicle <- SCC[source.MotorVehicle ,]$SCC
source.MotorVehicle.NEI <- NEI[NEI$SCC %in% source.MotorVehicle,]
# Get data in the city of Baltimore
source.MotorVehicle.baltimore <- subset(source.MotorVehicle.NEI,fips == "24510" )
# Calculate total emissions per year in Blatimore from Motor Vehicle sources
source.MotorVehicle.peryear.baltimore <- tapply(source.MotorVehicle.baltimore$Emissions, source.MotorVehicle.baltimore$year,sum)
# open device
png(filename='Plot5.png')
# Plot motor vehicle-related emissions in Baltimore per year
barplot(source.MotorVehicle.peryear.baltimore, main = "Total PM2.5 emission from Motor Vehicle sources in Baltimore", xlab = "Year", ylab="Emissions (Tons)", col='red')
